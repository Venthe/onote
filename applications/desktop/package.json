{
  "name": "@onote/desktop",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "copy:assets": "mkdir -p ./dist && cp -r ./public/* ./dist/",
    "clean": "rm -rf ./dist/*",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm --silent run clean && npm --silent run copy:assets && npm --silent run build && electron ./dist/main.js"
  },
  "author": "Jacek \"Venthe\" Lipiec",
  "license": "MIT",
  "dependencies": {
    "@onote/web-root": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "electron": "^26.2.2",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  }
}
